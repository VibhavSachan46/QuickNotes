<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vibhav.quicknotes">

    <application
        android:name="com.vibhav.quicknotes.NoteApp"
        android:allowBackup="true"
        android:icon="@drawable/notes"
        android:label="@string/app_name"
        android:roundIcon="@drawable/notes"
        android:supportsRtl="true"
        android:theme="@style/Theme.CleanArchitectureNoteApp">
        <activity
            android:name="com.vibhav.quicknotes.feature_note.presentation.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.CleanArchitectureNoteApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>